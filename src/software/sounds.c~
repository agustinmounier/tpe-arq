#include "../../include/kasm.h"
#include "../../include/syscall_hand.h"
#include "../../include/software.h"
void monkey_island_theme(){
	
	int freq1 = 500;
	int t1 = 2;
	_syscall(SET_FREQ, 0, freq1, 0);
	_syscall(PLAY_SPEAKER, 0, t1, 0);
	_syscall(PLAY_SPEAKER, 0, t1, 0);
	
	_syscall(PLAY_SPEAKER, 0, 1, 0);

}
